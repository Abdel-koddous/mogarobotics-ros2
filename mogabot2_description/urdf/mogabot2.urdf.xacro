    <joint name="world_base_joint" type="fixed">
        <parent link="world"/>
        <child link="base_link"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>
    
    <joint name="base_shoulder_joint" type="revolute">
        <parent link="base_link"/>
        <child link="shoulder_link"/>
        <origin xyz="0 0 0.81" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-1.57" upper="1.57" effort="30" velocity="10"/>
    </joint>

    <joint name="shoulder_arm1_joint" type="revolute">
        <parent link="shoulder_link"/>
        <child link="arm1_link"/>
        <origin xyz="0.266 -0.2872 0.8451" rpy="0 0 0"/>
        <axis xyz="1 0 0"/>
        <limit lower="-3.14" upper="3.14" effort="30" velocity="10"/>
    </joint>

    <joint name="arm1_elbow_joint" type="revolute">
        <parent link="arm1_link"/>
        <child link="elbow_link"/>
        <origin xyz="0.224 0 2.0" rpy="0 0 3.14"/>
        <axis xyz="1 0 0"/>
        <limit lower="-3.14" upper="3.14" effort="30" velocity="10"/>
    </joint>

    <joint name="elbow_forarm_joint" type="revolute">
        <parent link="elbow_link"/>
        <child link="forarm_link"/>
        <origin xyz="0.42 0.70 0" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="-3.14" upper="3.14" effort="30" velocity="10"/>
    </joint>

    <joint name="forarm_wrist_joint" type="revolute">
        <parent link="forarm_link"/>
        <child link="wrist_link"/>
        <origin xyz="0 1.584 0" rpy="0 0 0"/>
        <axis xyz="1 0 0"/>
        <limit lower="-3.14" upper="3.14" effort="30" velocity="10"/>
    </joint>

    <joint name="wrist_hand_joint" type="revolute">
        <parent link="wrist_link"/>
        <child link="hand_link"/>
        <origin xyz="0 0.4727 0" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="-3.14" upper="3.14" effort="30" velocity="10"/>        
    </joint>
